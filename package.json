{
  "name": "sonolus-taiko-engine",
  "version": "1.0.6",
  "description": "A recreation of Taiko no Tatsujin engine in Sonolus",
  "author": "NonSpicyBurrito",
  "repository": "github:NonSpicyBurrito/sonolus-taiko-engine",
  "license": "MIT",
  "keywords": [
    "Sonolus"
  ],
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "sonolus-cli --dev",
    "prebuild": "tsc -p ./lib/tsconfig.json --noEmit && tsc -p . --noEmit && eslint --ext .mts ./src && eslint --ext .cts ./lib",
    "build": "tsc -p ./lib/tsconfig.json && sonolus-cli --build && node ./lib/build.mjs"
  },
  "dependencies": {
    "sonolus-core": "~7.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.38",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "sonolus.js": "~9.0.2",
    "typescript": "~5.1.6"
  }
}
